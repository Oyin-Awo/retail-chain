                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">IF (SELECT count(*) from TescaEDW.EDW.FactPurchaseAnalysis) &lt;= 0
	SELECT 
	P.TransactionID, 
	P.TransactionNo, 
	convert(date, P.TransDate) TransDate, 
	Convert(date, OrderDate) OrderDate, 
	Convert(date, DeliveryDate) DeliveryDate, 
	DATEDIFF(DAY, P.orderdate, P.deliverydate) + 1 as DifferentialDays,
	P.VendorID, 
	P.EmployeeID, 
	P.ProductID,
	P.StoreID,
	P.Quantity,
	P.TaxAmount,
	P.LineAmount,
	getdate() as LoadDate
	FROM PurchaseTransaction P
	WHERE Convert(date, p.TransDate) &lt;= dateadd(day, -1, convert(date,getdate()))
	--Run query for data where TransDate is yesterday if there's already data in the DWH
ELSE
	SELECT 
	P.TransactionID, 
	P.TransactionNo, 
	convert(date, P.TransDate) TransDate, 
	Convert(date, OrderDate) OrderDate, 
	Convert(date, DeliveryDate) DeliveryDate, 
	DATEDIFF(DAY, P.orderdate, P.deliverydate) + 1 as DifferentialDays,
	P.VendorID, 
	P.EmployeeID, 
	P.ProductID,
	P.StoreID,
	P.Quantity,
	P.TaxAmount,
	P.LineAmount,
	getdate() as LoadDate
	FROM PurchaseTransaction P
	WHERE Convert(date, p.TransDate) = dateadd(day, -1, convert(date,getdate()))</property>
